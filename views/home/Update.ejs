<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../layout/_css.ejs') %>
    <title>Home</title>
</head>

<body>
    <%- include('../layout/_navbar.ejs') %>
    <div class="container mt-4">
        <form action="/HomeUpdate" method="post">
            <input type="hidden" name="id" value="<%= model.id%>">

            <label for="">Title</label>
            <input type="text" class="form-control" name="title" value="<%= model.title%>">
            <br>
            <label>SubTitle</label>
            <input type="text" class="form-control" name="subtitle" value="<%= model.subtitle%>">
            <br>
            <button class="btn btn-dark" type="submit">Save</button>
            <a href="/home//GetHomeList/" class="btn btn-outline-success">Go to list</a>
        </form>
    </div>
    <%- include('../layout/_js.ejs') %>
</body>

</html>